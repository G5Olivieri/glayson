<%= form_with model: note do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title, autofocus: 'autofocus' %>
  </div>
  <div>
    <%= form.label :body %>
    <%= form.text_area :body, rows: '30', style: 'width: 98%' %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
